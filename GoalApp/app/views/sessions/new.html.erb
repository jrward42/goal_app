<h1>Sign In</h1>
<form action="<%= session_url %>" method="post">
    <%= auth_token %>
    <label for "Username">
      Username
      <input type="text" name="user[username]" id = "Username">
    </label>
    <br>
    <label>
      Password
      <input type="password" name="user[password]">
    </label>
    <br>
    <input type="submit" value="Sign in">
</form>

<%= button_to 'Sign Up', new_user_url, method: :get %>
