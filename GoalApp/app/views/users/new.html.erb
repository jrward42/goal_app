<h1>Sign up</h1>
<form action="<%= users_url %>" method="post">
    <%= auth_token %>
    <label for "Username">
      Username
      <input type="text" name="user[username]" id = "Username">
    </label>
    <br>
    <label>
      Password
      <input type="text" name="user[password]">
    </label>
    <br>
    <input type="submit" value="Sign up" >
</form>
